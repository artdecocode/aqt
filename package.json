{
  "name": "@rqt/aqt",
  "version": "1.0.0",
  "description": "Advanced request for Node.js which returns body, headers and status after gzip.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "zoroaster -b test/spec",
    "test-build": "BABEL_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "doc": "doc documentary -o README.md",
    "lint": "eslint .",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "b": "b --source-maps",
    "build": "yarn-s b doc"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/aqt.git"
  },
  "keywords": [
    "request"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/aqt/issues"
  },
  "homepage": "https://github.com/rqt/aqt#readme",
  "devDependencies": {
    "documentary": "1.10.0",
    "eslint-config-artdeco": "1.0.1",
    "https-context": "1.0.2",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  }
}
