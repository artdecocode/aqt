{
  "name": "aqt",
  "version": "1.0.0",
  "description": "Advanced request for Node.js which returns body, headers and status after gzip.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -b",
    "test": "zoroaster -b test/spec",
    "test-build": "BABEL_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "build": "babel src --out-dir build --source-maps"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/aqt.git"
  },
  "keywords": [
    "aqt"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/aqt/issues"
  },
  "homepage": "https://github.com/artdecocode/aqt#readme",
  "devDependencies": {
    "eslint-config-artdeco": "1.0.0",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  }
}
