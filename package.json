{
  "name": "@rqt/aqt",
  "version": "1.2.6",
  "description": "An Advanced Network Request Package That Returns Body (Parsed As JSON If Needed), Headers And Status After Gzip Decompression.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "compile": "depack-dev src/depack -o compile/aqt.js -c -a -p -s --source_map_include_content",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "d": "yarn-s d1 dc",
    "d1": "typal types/index.js types/externs.js",
    "dc": "typal compile/index.js -c -t types/index.xml",
    "lint": "eslint .",
    "e": "alanode",
    "example/": "yarn e example/example.js"
  },
  "files": [
    "compile",
    "src",
    "types"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/aqt.git"
  },
  "keywords": [
    "request",
    "network",
    "http",
    "https",
    "headers",
    "body",
    "json",
    "gzip"
  ],
  "alamode": true,
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/aqt/issues"
  },
  "homepage": "https://github.com/rqt/aqt#readme",
  "devDependencies": {
    "@idio/idio": "^0.1.0",
    "alamode": "^2.3.4",
    "documentary": "^1.27.4",
    "eslint-config-artdeco": "1.0.1",
    "https-context": "^1.0.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha",
    "catchment": "^3.3.0",
    "erotic": "^2.1.1"
  },
  "srcDependencies": ["catchment", "erotic"]
}
