{
  "name": "@rqt/aqt",
  "version": "1.0.2",
  "description": "Advanced request for Node.js which returns body, headers and status after gzip.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "d": "NODE_DEBUG=doc doc src/index.js -g",
    "lint": "eslint .",
    "e": "node example",
    "example/": "yarn e example/example.js",
    "b": "alamode src -o build",
    "build": "yarn-s b doc"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/aqt.git"
  },
  "keywords": [
    "request",
    "http",
    "https",
    "headers",
    "body",
    "json",
    "gzip"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/aqt/issues"
  },
  "homepage": "https://github.com/rqt/aqt#readme",
  "devDependencies": {
    "documentary": "1.19.0",
    "eslint-config-artdeco": "1.0.1",
    "https-context": "1.0.3",
    "yarn-s": "1.1.0",
    "zoroaster": "3.5.1"
  }
}
