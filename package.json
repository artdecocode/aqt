{
  "name": "@rqt/aqt",
  "version": "1.2.5",
  "description": "An Advanced Network Request Package That Returns Body (Parsed As JSON If Needed), Headers And Status After Gzip Decompression.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "d": "typal src/index.js -c -g",
    "lint": "eslint .",
    "e": "alanode",
    "example/": "yarn e example/example.js",
    "b": "alamode src -o build -s",
    "build": "yarn-s d b doc"
  },
  "files": [
    "build",
    "types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rqt/aqt.git"
  },
  "keywords": [
    "request",
    "network",
    "http",
    "https",
    "headers",
    "body",
    "json",
    "gzip"
  ],
  "alamode": true,
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rqt/aqt/issues"
  },
  "homepage": "https://github.com/rqt/aqt#readme",
  "devDependencies": {
    "@idio/idio": "^0.1.0",
    "alamode": "^2.3.4",
    "documentary": "^1.27.4",
    "eslint-config-artdeco": "1.0.1",
    "https-context": "^1.0.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "catchment": "^3.3.0",
    "erotic": "^2.1.1"
  }
}
